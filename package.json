{
  "name": "graphql-jit",
  "version": "0.1.0",
  "description": "GraphQL JIT Compiler to JS",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Rui Araujo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:ruiaraujo/graphql-jit.git"
  },
  "scripts": {
    "prepublishOnly": "npm run test && npm run build",
    "build": "tslint --project . && tsc",
    "test": "jest",
    "test-cov": "jest --coverage"
  },
  "files": [
    "dist/*"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "!**/__tests__/*",
      "!**/__benchmarks__/*",
      "**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "^.+\\.d\\.ts$"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 87,
        "functions": 93,
        "lines": 94,
        "statements": 94
      }
    },
    "coverageReporters": [
      "text",
      "text-summary",
      "html"
    ],
    "testRegex": "(/tests/.*|(\\.|/)test)\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "peerDependencies": {
    "fast-json-stringify": "^1.9.1",
    "graphql": "^0.13.2"
  },
  "devDependencies": {
    "@types/benchmark": "^1.0.31",
    "@types/graphql": "^0.13.4",
    "@types/jest": "^23.3.5",
    "@types/json-schema": "^7.0.1",
    "@types/lodash.merge": "^4.6.4",
    "@types/node": "^10.11.7",
    "benchmark": "^2.1.4",
    "fast-json-stringify": "^1.9.1",
    "graphql": "^0.13.2",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.0",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "json-schema": "^0.2.3",
    "lodash.merge": "^4.6.1"
  }
}
